{
	"name": "LDAPAuthentication",
	"version": "1.0.0-alpha",
	"author": [
		"Cindy Cicalese",
		"[http://mediawiki.org/wiki/User:MarkAHershberger Mark A. Hershberger]",
		"[http://mediawiki.org/wiki/User:Osnard Robert Vogel]"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:LDAPAuthentication",
	"descriptionmsg": "ldapauthentication-desc",
	"license-name": "GPL-2.0",
	"type": "other",
	"requires": {
		"extensions": {
			"LDAPProvider": "*",
			"PluggableAuth": "*"
		}
	},
	"MessagesDirs": {
		"LDAPAuthentication": [
			"i18n"
		]
	},
	"ExtensionFunctions": [
		"MediaWiki\\Extensions\\LDAPAuthentication\\Setup::init"
	],
	"config": {
		"PluggableAuth_Class": {
			"value": "MediaWiki\\Extensions\\LDAPAuthentication\\PluggableAuth"
		}
	},
	"attributes": {
		"LDAPProvider": {
			"DefaultSettings": {
				"authentication": {
					"usernameattribute": "samaccountname",
					"realnameattribute": "cn",
					"emailattribute": "mail"
				}
			}
		}
	},
	"load_composer_autoloader" : true,
	"manifest_version": 2
}
